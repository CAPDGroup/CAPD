	TEMPLATE_EMPTY
	template<bool r> inline float filib::fp_traits<float,filib::no_rounding>::upward_plus(
			float const & op1,
			float const & op2
		)
	{
		float op3 = op1+op2;
		if ( r ) reset();
		return op3;
	}
